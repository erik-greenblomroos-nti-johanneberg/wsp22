div.auction_banner
    h1.banner_title Auction House 
    div.sell_button Sell 
    div.balance balance 
    div.burger |||
div.auction_table

    - result.each do |nft|
        .auction_container
            .nft_container
                img.nft_owned src="/img/#{nft["URL"]}" alt="#{nft["Description"]}" /
            .auction_interaction
                .small_display
                    p $ #{nft["Startprice"]}
                .small_display
                    p #{nft["Name"]}
                .small_display
                    p 2h left
                .small_display
                    .tiny_display 
                    form action="/auction/#{nft["Id"]}/bid" method="POST"
                        input type="submit" value="Bid" 
                    .tiny_display  
                        p Hist  

